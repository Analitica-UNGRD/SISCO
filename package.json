{
	"name": "seguimiento_contratistas",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "concurrently -k \"npm:proxy\" \"wait-on http://localhost:3000/health && npx http-server . -p 5500 -c-1\"",
		"proxy": "node server.js",
		"check:no-backups": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/check-no-backups.ps1"
	},
	"dependencies": {
		"body-parser": "^1.20.2",
		"express": "^4.18.2",
		"node-fetch": "^2.6.9"
	},
	"devDependencies": {
		"concurrently": "^9.2.1",
		"wait-on": "^8.0.4"
	}
}
